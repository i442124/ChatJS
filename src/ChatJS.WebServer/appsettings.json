{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "IdentityServer": {
    "Clients": {
      "ChatJS.WebApp": {
        "Profile": "SPA",
        "RedirectUri": "http://localhost:3000/authentication/login-callback",
        "LogoutUri": "http://localhost:3000/authentication/logout-callback"
      }
    },
    "Key": {
      "Type": "Development"
    }
  },
  "AllowedHosts": "*"
}
